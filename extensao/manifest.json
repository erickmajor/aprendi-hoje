{
   "browser_action": {
      "default_popup": "index.html"
   },
   "description": "Plugin para controle de links da semana.",
   "manifest_version": 2,
   "name": "Links da semana - gerencie seus links semanalmente.",
	"icons": { 	"16": "img/icon16.png",
           		"48": "img/icon48.png",
          		"128": "img/icon128.png"},   
   "permissions": [ "notifications", "tabs", "http://*/*", "https://*/*" ],
   "version": "0.0.0.1"
}
